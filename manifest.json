{
  "manifest_version": 2,
  "name": "Semantic Search Extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "permissions": ["activeTab"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
